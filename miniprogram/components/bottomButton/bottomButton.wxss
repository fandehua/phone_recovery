.btn-box{
  z-index: 99;
  height: 120rpx;
  width: 100vw;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  box-shadow: 0px -1px 5px rgba(0, 0, 0, 0.1);
}
.btn-unDisabled{
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #e64340;
  color: #fff;
  font-weight: 300;
}
.btn-disabled{
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  color: #fff !important;
  background: #eeb1b0 !important;
  font-weight: 300;
}